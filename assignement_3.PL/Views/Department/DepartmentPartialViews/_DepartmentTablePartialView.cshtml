<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Date Of Creation</th>
            <th>Details</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ITem in Model)
        {
            <tr>
                <td>
                    @ITem.Code
                </td>
                <td>
                    @ITem.Name
                </td>
                <td>
                    @ITem.CreateAt
                </td>
                <partial name="BtnPartialView" model="@ITem.Id.ToString()" />

            </tr>
        }
    </tbody>
</table>